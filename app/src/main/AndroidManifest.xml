            android:value="E1v9TSMLLit2VHTTl0i9"/>
        <meta-data
            android:name="ZBP_APP_ENV"
            android:value="dev"/>

        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>

        <activity android:name=".views.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.QuickLoginActivity"/>
        <activity android:name=".views.activities.BindPhoneActivity"/>
        <activity android:name=".views.activities.ModifyPhoneActivity"/>
